<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<!-- Environment Initialization -->
	<bean id="geotiffGeoServerGeneratorService" class="it.geosolutions.geobatch.geoserver.geotiff.GeoTIFFGeoServerGeneratorService">
		<property name="id" value="geotiffGeoServerGeneratorService"/>
    	<property name="description" value="geotiffGeoServerGeneratorService"/>
        <property name="name" value="geotiffGeoServerGeneratorService"/>
    </bean>	
    
    <bean id="geotiffFolderGeoServerConfiguratorService" class="it.geosolutions.geobatch.geoserver.geotiff.GeoTIFFFolderGeoServerConfiguratorService">
		<property name="id" value="geotiffFolderGeoServerConfiguratorService"/>
    	<property name="description" value="geotiffFolderGeoServerConfiguratorService"/>
    	<property name="name" value="geotiffFolderGeoServerConfiguratorService"/>
    </bean>

	<!-- Environment Initialization -->
	<bean id="geotiffOverviewsEmbedderService" class="it.geosolutions.geobatch.geotiff.overview.GeoTiffOverviewsEmbedderService">
		<property name="id" value="geotiffOverviewsEmbedderService"/>
    	<property name="description" value="geotiffOverviewsEmbedderService"/>
    	<property name="name" value="geotiffOverviewsEmbedderService"/>
    </bean>	    
        
	<!-- Environment Initialization -->
	<bean id="geoTiffRetilerService" class="it.geosolutions.geobatch.geotiff.retile.GeoTiffRetilerService">
		<property  name="id" value="geoTiffRetilerService"/>
    	<property name="description" value="geoTiffRetilerService"/>
    	<property name="name" value="geoTiffRetilerService"/>
    </bean>	      

    <bean id="flowGeoTIFFRegistrar" class="it.geosolutions.geobatch.geotiff.GeoTIFFAliasRegistrar" lazy-init="false">
        <constructor-arg ref="aliasRegistry" />
    </bean>

</beans>
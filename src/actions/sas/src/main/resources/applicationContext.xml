<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<!-- Environment Initialization -->
	<bean id="FormatConverterService" class="it.geosolutions.geobatch.sas.convert.FormatConverterService">
		<property  name="id">
    		<value>FormatConverterService</value>
    	</property>
    	<property name="description">
    		<value>FormatConverterService</value>
    	</property>
    	<property name="name">
    		<value>FormatConverterService</value>
    	</property>
    </bean>	    
    
	<!-- Environment Initialization -->
	<bean id="MosaicerService" class="it.geosolutions.geobatch.sas.mosaic.MosaicerService">
		<property  name="id">
    		<value>MosaicerService</value>
    	</property>
    	<property name="description">
    		<value>MosaicerService</value>
    	</property>
    	<property name="name">
    		<value>MosaicerService</value>
    	</property>
    </bean>	      
    
    <!-- Environment Initialization -->
	<bean id="ComposerService" class="it.geosolutions.geobatch.sas.compose.SASComposerService">
		<property name="id">
    		<value>ComposerService</value>
    	</property>
    	<property name="description">
    		<value>ComposerService</value>
    	</property>
    	<property name="name">
    		<value>ComposerService</value>
    	</property>
    </bean>	     
    
     <!-- Environment Initialization -->
	<bean id="DetectionManagerService" class="it.geosolutions.geobatch.sas.detection.SASDetectionService">
		<property name="id">
    		<value>DetectionManagerService</value>
    	</property>
    	<property name="description">
    		<value>DetectionManagerService</value>
    	</property>
    	<property name="name">
    		<value>DetectionManagerService</value>
    	</property>
    </bean>	     

     <!-- Environment Initialization -->
	<bean id="sasEsbService" class="it.geosolutions.geobatch.sas.esb.SasEsbService">
		<property name="id" value="sasEsbService"/>
    	<property name="description" value="sasEsbService"/>
    	<property name="name" value="sasEsbService"/>
    </bean>	 

	<!-- Aliases Registrars -->
    <bean id="sasRegistrar" class="it.geosolutions.geobatch.sas.SASAliasRegistrar" lazy-init="false">
        <constructor-arg ref="aliasRegistry" />
    </bean>
    <bean id="sasEsbRegistrar" class="it.geosolutions.geobatch.sas.SasEsbAliasRegistrar" lazy-init="false">
        <constructor-arg ref="aliasRegistry" />
    </bean>
    
</beans>

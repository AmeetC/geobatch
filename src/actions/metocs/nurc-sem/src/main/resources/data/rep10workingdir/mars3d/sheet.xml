<!-- OCTAVE ENV -->
<octave>
  <sheets>
	<!-- OCTAVE SHEET -->
	<!-- This sheet can be used with all the meteoam octave scripts-->
	<FunctionSheet name="fileInFileOut_mars3d">
	  <commands/>
	  <definitions/>
	  <functions>
		<function name="${FUNCTION}">
		  <commands>
			  <OctaveCommand executed="false">
				<command>source "${SOURCEDIR}";</command>
			</OctaveCommand>
			<OctaveCommand executed="false">
				<command>cd "${WORKINGDIR}/"</command>
			</OctaveCommand>
	      </commands>
		  <definitions>
			<OctaveFile name="file_in" value="${IN_FILE}" output="false" input="true"/>
			<OctaveFile name="file_out" value="${OUT_FILE}" output="false" input="true"/>
		  </definitions>
		  <returns/>
		</function>
	  </functions>
	  <returns/>
	</FunctionSheet>
  </sheets>
</octave>

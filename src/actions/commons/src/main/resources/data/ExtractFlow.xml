<?xml version="1.0" encoding="UTF-8"?>
<FlowConfiguration>
	
	<id>ExtractFlow</id>
	<description>ExtractFlow</description>
	<name>ExtractFlow</name>
	
	<autorun>true</autorun>
	
	<EventGeneratorConfiguration>
		<wildCard>*.*</wildCard>
		<watchDirectory>in</watchDirectory>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
		<interval>0 * * * * ?</interval><!-- CRON: every minute at sec 0 -->
		<!--<interval>20</interval> each 20 sec -->
		<id>Flow</id>
		<name>test</name>
	</EventGeneratorConfiguration>
	
	<EventConsumerConfiguration>
		<id>extract</id>
	
		<performBackup>false</performBackup>
		
		<ExtractConfiguration>
			<serviceID>ExtractGeneratorService</serviceID>
			
			<id>extract</id>
			<description>description</description>
			<name>ExtractConfiguration</name>
		  
			<dirty>false</dirty>
			<listenerConfigurations/>
			<failIgnored>false</failIgnored>
			
			<destination>out/</destination>
			
			<!-- Static data into DataModel --> 
			<!--
			<root>
				<entry>
					<string>KEY</string>
					<string>value</string>
				</entry>
			</root>
			 -->
		</ExtractConfiguration>
		
	</EventConsumerConfiguration>

</FlowConfiguration>
